#!/bin/bash

echo "Adding UUID based off of created time to the beginning of file name"

for f in $1/*; do 
  if [[ ! -d $f ]]; then
    ct=${f:2:12}
    sfct="${ct:4:2}/${ct:6:2}/${ct:0:4} ${ct:8:2}:${ct:10:2}"
    uuid=$(stat -f %SB -t %Y%m%d%H%M "$f")
    echo "Adding $uuid to beginning of ${f:2}"
    mv "${f:2}" "$uuid ${f:2}"
  fi
done
